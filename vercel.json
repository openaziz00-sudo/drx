{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "devCommand": "cd gentle-ai-backend-nodejs && npm start",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "gentle-ai-backend-nodejs/public",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
